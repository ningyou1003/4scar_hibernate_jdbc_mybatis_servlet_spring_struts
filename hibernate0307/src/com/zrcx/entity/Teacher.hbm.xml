<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.zrcx.entity">
	<class name="Teacher" table="T_HBM_TEACHER">
		<id name="id" column="ID">
			<!-- 配置ID生成器 -->
			<generator class="increment"/><!-- 组件，新增的时候用，查找最大的ID -->
		</id>		<!-- 实体类的属性 ,数据库的名字 -->
		<property name="name" column="NAME" />
		<property name="sex" column="sex" />
		<property name="birthday" column="birthday" />
		<property name="workDate" column="work_Date" />
		<property name="specialty" column="specialty" />
		<property name="level" column="level" />
		<property name="createDate" column="CREATE_DATE" />
	</class>
</hibernate-mapping>

















