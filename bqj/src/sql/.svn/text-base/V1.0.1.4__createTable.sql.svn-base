--新建简报表
CREATE TABLE `p_bulletin` (
	`KeyID` varchar(50) NOT NULL DEFAULT '',
	`Title` varchar(255) NULL COMMENT '标题',
	`Content` varchar(255) NULL COMMENT '内容',
	`Attachment` varchar(255) NULL COMMENT '附件',
	PRIMARY KEY (`KeyID`) 
)ENGINE=InnoDB DEFAULT CHARSET=utf8;
