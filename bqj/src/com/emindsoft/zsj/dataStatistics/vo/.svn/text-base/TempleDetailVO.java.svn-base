package com.emindsoft.zsj.dataStatistics.vo;

import java.util.List;

import com.emindsoft.zsj.base.attachment.model.Attachment;
import com.emindsoft.zsj.dataStatistics.model.Temple;

public class TempleDetailVO {
	
	private Temple temple;
	private List<Attachment> attachment;
	
	public TempleDetailVO(Temple temple,List<Attachment> attachment) {
		this.temple = temple;
		this.attachment = attachment;
	}
	
	public Temple getTemple() {
		return temple;
	}
	public void setTemple(Temple temple) {
		this.temple = temple;
	}
	public List<Attachment> getAttachment() {
		return attachment;
	}
	public void setAttachment(List<Attachment> attachment) {
		this.attachment = attachment;
	}

}
