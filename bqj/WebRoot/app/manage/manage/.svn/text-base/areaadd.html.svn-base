							<li class="row-item"><label for="AreaName"> 名称 ：</label>
								<input name="areaName" type="text" id="areaName" maxlength="15"
									   ms-duplex="data.region" class="dfinput" data-rule="required;"
									   placeholder="请输入15个汉字以内的区域名称" /><font color="red">*</font>
							</li>
						</ul> 
						<br />
						<br /></td>
				</tr>
				
				<tr>
					<td>
						<ul class="forminfo">
							<li class="row-item"><label for="AreaName"> 位于 ：</label>
								<span id="parentArea"></span>
							</li>
						</ul> 
						<br />
						<br /></td>
				</tr>
				
