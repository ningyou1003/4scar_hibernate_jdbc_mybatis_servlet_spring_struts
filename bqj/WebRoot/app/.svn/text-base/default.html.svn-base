    <link href="../assets/js/events/css/style.css" rel="stylesheet" type="text/css" />
    <style>
        #tagsW
        {
            padding: 0px 0 0 0;
            margin: 0px;
            width: auto;
            height: 40px;
        }
        #tagsW li
        {
            margin-left: 0px;
            float: left;
            margin-right: 0px;
            list-style-type: none;
            width: 150px;
            line-height: 37px;
            height: 37px;
            color: #000;
            text-align: center;
            border-right: solid 1px #d3dbde;
        }
        #tagsW li a
        {
            float: left;
            color: #000;
            width: 150px;
            line-height: 37px;
            height: 37px;
            text-decoration: none;
            font-size: 14px;
            font-weight: 500;
            text-align: center;
            display: inline-block;
        }
        #tagsW li.selectTagW
        {
            background-position: left top;
            height: 37px;
            background: #fff;
            width: 150px;
        }
        #tagsW li.selectTagW a
        {
            color: #000;
            font-weight: bold;
            line-height: 37px;
            height: 37px;
            font-size: 14px;
        }
        #tagContentW
        {
            padding: 0px;
            display: block;
            height: auto;
        }
        .tagContentW
        {
            display: none;
            height: auto;
            padding: 10px;
            width: 98%;
        }
        #tagContentW .selectTagW
        {
            display: block;
        }
        #inform
        {
            background-color: #FDFFE5;
            border: solid #CCC 1px;
            padding-left: 10px;
            height: 28px;
            padding-top: 10px;
            margin-right: 5px;
            padding-right: 5px;
        }
    </style>
</head>
<body ms-controller="flowCtrl">
    <form name="form1" method="post" action="" id="form1" enctype="multipart/form-data">
    <div class="mainbox">
        <div>
            <div style="width: 99%;">
                <div style="width: 90%; min-width: 1070px; max-width: 1070px; float: left;">
                    <div class="leftinfo" style="height: 460px;">
                        <div class="listtitle">
                            最新动态 <a href="javascript:" onclick="openTabUrl('最新动态','news/dynamicManage/dynamicList.html')"
                                class="more1" style="color: #b8211b; font-size: 12px">更多</a></div>
                        <div class="mncon1" style="height: 384px; float: left;">
                            <div class="mc1L">
                                <div class="mc1_focus">
                                    <div id="slideBox" class="slideBox slideBox1">
                                        <div class="mc1_btnL prev">
                                        </div>
                                        <div class="mc1_btnR next">
                                        </div>
                                        <div class="hd">
                                            <ul id="hdlist">
                                            </ul>
                                        </div>
                                        <div class="bd">
                                            <ul id="imgnewlist">
                                            </ul>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="mc1R">
                                <div class="mc1r_t2" id="firstnew">
                                </div>
                                <p id="firstnewcontent" style="font-size: 12px">
                                </p>
                                <div class="mc1r_u1" style="margin-top: 10px">
                                    <ul id="txtnewlist">
                                    </ul>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="leftinfo" id="leftinfoC" style="height: 100%; padding-bottom: 20px" onmouseover="zhuan()"
                        onmouseout="jixu()">
                        <script language="JavaScript" type="text/JavaScript">
                            function selectTagW(showContent, selfObj) {
                                var tag = document.getElementById("tagsW").getElementsByTagName("li");
                                var taglength = tag.length;
                                for (i = 0; i < taglength; i++) {
                                    tag[i].className = "";
                                }
                                selfObj.parentNode.className = "selectTagW";
                                for (i = 0; j = document.getElementById("tagContentW" + i); i++) {
                                    j.style.display = "none";
                                    if ("tagContentW" + i == showContent) window.GridsumSnapshotID = i;
                                }
                                document.getElementById(showContent).style.display = "block";
                            }
                            var x = 0;

                            function scrollTag() {
                                var tags = document.getElementById("tagsW").getElementsByTagName("a");
                                if (x < 4) { x = x + 1 }
                                else
                                    x = 0;
                                var tag = document.getElementById("tagsW").getElementsByTagName("li");
                                var taglength = tag.length;
                                for (i = 0; i < taglength; i++) {
                                    tag[i].className = "";
                                }
                                tags[x].parentNode.className = "selectTagW";
                                for (i = 0; j = document.getElementById("tagContentW" + i); i++) {
                                    j.style.display = "none";
                                }
                                document.getElementById("tagContentW" + x).style.display = "block";
                            }
                            var scrolll = setInterval(scrollTag, 3000);
                            function zhuan() {
                                clearInterval(scrolll);
                            }
                            function jixu() {
                                scrolll = setInterval(scrollTag, 3000);
                            } 
                        </script>
                        <div class="listtitle">
                            <ul id="tagsW">
                                <li class="selectTagW"><a href="JavaScript:void()" onmousemove="selectTagW('tagContentW0',this)"
                                    onclick="openManageTabUrl()">领导小组和办公室</a></li>
                                <li><a href="JavaScript:void()" onmousemove="selectTagW('tagContentW1',this)" onclick="openManageTabUrl()">
                                    成员单位及其职责</a></li>
                                <li><a href="JavaScript:void()" onmousemove="selectTagW('tagContentW2',this)" onclick="openManageTabUrl()">
                                    文化综合执法队伍</a></li>
                                <li><a href="JavaScript:void()" onmousemove="selectTagW('tagContentW3',this)" onclick="openTabUrl('大事记','news/eventsManage/eventsList.html')">
                                    大事记</a></li>
                            </ul>
                        </div>
                        <div id="tagContentW">
                            <div id="tagContentW0" class="tagContentW selectTagW">
                                <div style="height: auto;">
                                    <table style="width: 100%">
                                        <tr>
                                            <td valign="top">
                                                <h3 style="font-size: 15px; padding-left: 10px">
                                                    领导小组</h3>
                                                <div id="ldxz" style="padding: 10px">
                                                </div>
                                            </td>
                                            <td valign="top">
                                                <h3 style="font-size: 15px; padding-left: 10px">
                                                    办公室</h3>
                                                <div id="bgs" style="padding: 10px">
                                                </div>
                                            </td>
                                        </tr>
                                    </table>
                                </div>
                            </div>
                            <div id="tagContentW1" class="tagContentW">
                                <div style="height: auto;">
                                    <table style="width: 100%">
                                        <tr>
                                            <td valign="top">
                                                <div id="cydwzz" style="padding: 10px;">
                                                </div>
                                            </td>
                                        </tr>
                                    </table>
                                </div>
                            </div>
                            <div id="tagContentW2" class="tagContentW">
                                <div style="height: auto;">
                                    <table style="width: 100%">
                                        <tr>
                                            <td valign="top">
                                                <div id="zfd" style="padding: 10px">
                                                </div>
                                            </td>
                                        </tr>
                                    </table>
                                </div>
                            </div>
                            <div id="tagContentW3" class="tagContentW">
                                <div style="height: auto;">
                                    <table style="width: 100%">
                                        <tr>
                                            <td valign="top">
                                                <div class="box">
                                                    <ul class="event_list" id="listEvents">
                                                    </ul>
                                                </div>
                                            </td>
                                        </tr>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div style="height: 60px; width: 100%; margin-top: 8px; float: left;">
                    </div>
                </div>
                <div style="width: 10%; min-width: 90px; float: left">
                    <div class="leftinfo" style="margin-left: 10px; height: 770px; max-width: 300px;
                        min-width: 110px; width: 100%">
                        <div class="listtitle">
                            <a href="javascript:" class="more1">
                                <!--更多-->
                            </a>快速进入</div>
                        <ul class="newlist" ms-repeat="vm.manageData">
                            <li><a href="JavaScript:void()" ms-click="openManage(el.regioncode)" style="font-size: 12px">
                                {{el.region}}</a></li>
                            <!-- <li><a href="#" ms-click="xhArea(el.regioncode)">{{el.region}}</a></li> -->
                        </ul>
                        <div class="maintj">
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
