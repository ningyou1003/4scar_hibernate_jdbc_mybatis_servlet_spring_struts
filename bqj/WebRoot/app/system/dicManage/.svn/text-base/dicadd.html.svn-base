<body>
	<div ms-controller="dicaddCtrl">
		<form name="base_form" method="post" id="base_form">
<!-- 			<div class="place">
				<span>位置：</span>
				<ul class="placeul">
					<li>首页</li>
					<li>系统设置</li>
					<li>数据字典管理</li>
					<li>字典编辑</li>
				</ul>
			</div> -->
			<div class="formbody">
				<div class="formtitle">

					<span>基本信息</span>
				</div>
				<ul class="forminfo">
					<li class="row-item"><label> 名称值 ：</label><input
						name="txtDictionaryName" type="text" id="txtDictionaryName" maxlength="20"
						class="dfinput" ms-duplex="data.dictionaryname" data-rule="required;" /><b>*</b><i></i>
					</li>
					<li class="row-item"><label> 类型名称 ：</label><input
						name="txtDictionaryType" type="text" id="txtDictionaryType"
						class="searchBox" ms-duplex="data.dictionarytype" data-rule="required;" /><b>*</b><i></i>
					</li>
					<li class="row-item"><label> 排序 ：</label><input
						name="txtOrderID" type="text" id="txtOrderID"
						class="dfinput" ms-duplex="data.orderid"
						data-rule="required;positive;" placeholder="请输入整数" /><b>*</b>
					</li>
					<li><label> &nbsp;</label>
					    <input type="button" name="Button1"
						value="确认保存" id="Button1" ms-click="submit()" class="btn" />
						&nbsp;
						<input type="button" name="btback" value="返回" id="btback"
						class="btn" />
				</ul>
			</div>
		</form>
	</div>
</body>
