package com.emindsoft.zsj.docNotice.vo;

public class SummationsVO {
	private String time;
	private long sum;
	
	public String getTime() {
		return time;
	}

	public SummationsVO(String time, long sum) {
		super();
		this.time = time;
		this.sum = sum;
	}

	public void setTime(String time) {
		this.time = time;
	}

	public long getSum() {
		return sum;
	}

	public void setSum(long sum) {
		this.sum = sum;
	}

	public SummationsVO() {
		super();
	}
	
}
