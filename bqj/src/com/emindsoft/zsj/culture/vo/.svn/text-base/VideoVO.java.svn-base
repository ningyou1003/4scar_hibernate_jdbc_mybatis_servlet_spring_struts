package com.emindsoft.zsj.culture.vo;

import java.util.List;

import com.emindsoft.zsj.base.attachment.model.Attachment;
import com.emindsoft.zsj.culture.model.Video;

public class VideoVO {
	private Video video;
	private List<Attachment> attachment;
	
	public VideoVO(Video video,List<Attachment> attachment) {
		this.video = video;
		this.attachment = attachment;
	}
	
	public List<Attachment> getAttachment() {
		return attachment;
	}

	public void setAttachment(List<Attachment> attachment) {
		this.attachment = attachment;
	}

	public VideoVO(Video video) {
		super();
		this.video = video;
	}

	public Video getVideo() {
		return video;
	}

	public void setVideo(Video video) {
		this.video = video;
	}
	

}
